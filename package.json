{
  "name": "solid-ui",
  "version": "0.10.5",
  "description": "UI library for writing Solid read-write-web applications",
  "main": "./lib/index.js",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "build-lib": "babel src -d lib",
    "build": "npm run build-lib",
    "postversion": "git push origin master --follow-tags",
    "prepublishOnly": "npm run build",
    "standard": "standard",
    "test": "npm run standard"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/solid/solid-ui"
  },
  "keywords": [
    "solid",
    "decentralized",
    "widgets",
    "ui",
    "web",
    "rdf",
    "ldp",
    "linked",
    "data"
  ],
  "author": "Tim Berners-Lee <timbl@mit.edu>",
  "contributors": [
    "Daniel Friedman <danielf@mit.edu>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/solid/solid-ui/issues"
  },
  "homepage": "https://github.com/solid/solid-ui",
  "dependencies": {
    "escape-html": "^1.0.3",
    "node-uuid": "^1.4.7",
    "rdflib": ">=0.16.6",
    "regenerate": "^1.3.2",
    "solid-auth-client": "^1.0.0",
    "solid-auth-tls": "^0.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "standard": "^10.0.2"
  },
  "standard": {
    "globals": [
      "$rdf",
      "tabulator",
      "QUnit",
      "$SolidTestEnvironment",
      "AudioContext"
    ]
  }
}
